<div class="row mb-4">
  <div class=" col-md-6 offset-3">
    <%= render "users/user", user: @user %>
  </div>
</div>

<div class="row mb-2">
  <div class=" col-md-6 offset-3">
    <%= render "users/profile_nav", user: @user %>
  </div>
</div>

<h2>Own photos</h2>

<ul>
  <% @user.own_photos.each do |photo| %>
    <%= render "shared/photo", photo: photo %>
  <% end %>
</ul>
